    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.3.0",
    "@supabase/supabase-js": "^2.50.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "mongodb": "^6.17.0",
    "next": "15.2.4",
    "next-auth": "5.0.0-beta.25",
    "node-mailjet": "^6.0.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-to-print": "^3.1.0",
    "socket.io": "^4.8.1",
